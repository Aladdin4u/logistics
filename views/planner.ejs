<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="widtd=device-widtd, initial-scale=1.0" />
    <link rel="stylesheet" href="/css/style.css" />
    <title>LTC</title>
  </head>
  <body>
    <h1>Logistic Transportation Company</h1>
    <button><a href="/">home</a></button>
    <div>
      <!-- planner -->
      <table>
        <thead>
          <tr>
            <th>Date</th>
            <th>slot1</th>
            <th>slot2</th>
            <th>slot3</th>
            <th>slot4</th>
          </tr>
        </thead>
        <tbody>
          <% plan.forEach( el => { %>
          <tr>
            <td><%= el.order_date %></td>
            <td><%= el.slot_1_customer %></td>
            <td><%= el.slot_2_customer %></td>
            <td><%= el.slot_3_customer %></td>
            <td><%= el.slot_4_customer %></td>
          </tr>
          <% }) %>
        </tbody>
      </table>
    </div>

    <script src="js/main.js"></script>
  </body>
</html>
